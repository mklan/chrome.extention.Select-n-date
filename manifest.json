{
  "manifest_version": 2,

  "name": "Select-n-date",
  "description": "Create a new calendar event by selecting a date or a title from a website.",
  "version": "0.2",

  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "app.js",
      "lib/date.js"
    ]
  },
  "icons": { 
                "38":"images/icon38.png", 
                "128":"images/icon128.png" 
        }, 
  "browser_action": {
    "default_icon": {
      "38": "images/icon38.png"
    }
  }     
}